{
    "models": {
        "vqvae": {
            "name": "SparseSDFVQVAE",
            "args": {
                "resolution": 64,
                "model_channels": 512,
                "latent_channels": 32,
                "num_blocks": 8,
                "num_embeddings": 8192,
                "num_heads": null,
                "num_head_channels": 64,
                "mlp_ratio": 4.0,
                "attn_mode": "swin",
                "window_size": 8,
                "pe_mode": "ape",
                "use_fp16": true,
                "use_checkpoint": true,
                "qk_rms_norm": false,
                "representation_config": null
            }
        }
    },
    "dataset": {
        "name": "SparseSDF",
        "args": {
            "resolution": 1024,
            "min_points": 1000,
            "max_points": 2000000
        }
    },
    "trainer": {
        "name": "SparseSDF_VQVAETrainer",
        "args": {
            "max_steps": 500000,
            "batch_size_per_gpu": 1,
            "batch_split": 4,
            "optimizer": {
                "name": "AdamW",
                "args": {
                    "lr": 2e-5,
                    "weight_decay": 0.0
                }
            },
            "lr_scheduler": null,
            "ema_rate": [0.9999],
            "fp16_mode": "inflat_all",
            "fp16_scale_growth": 0.001,
            "grad_clip": {
                "name": "AdaptiveGradClipper",
                "args": {
                    "max_norm": 1.0,
                    "clip_percentile": 95
                }
            },
            "lambda_vq": 1.0,
            "lambda_commitment": 0.25,
            "loss_type": "mse",
            "pretrained_vae_path": "../Direct3D-S2-main/vae_weights/sparse_vae_1024.pth",
            "i_print": 1000,
            "i_log": 500,
            "i_sample": 10000,
            "i_save": 10000,
            "i_ddpcheck": 10000
        }
    }
}

