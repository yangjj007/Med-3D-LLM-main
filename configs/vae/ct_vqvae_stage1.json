{
    "models": {
        "vqvae": {
            "name": "SparseSDFVQVAE",
            "args": {
                "use_ema_update": false,
                "vq_decay": 0.99,
                "vq_epsilon": 1e-5,
                "use_kmeans_reinit": true,
                "kmeans_interval": 50,
                "reservoir_size": 16384,
                "resolution": 64,
                "model_channels": 512,
                "latent_channels": 16,
                "num_blocks": 8,
                "num_embeddings": 1024,
                "num_heads": null,
                "num_head_channels": 64,
                "mlp_ratio": 4.0,
                "attn_mode": "swin",
                "window_size": 8,
                "pe_mode": "ape",
                "use_fp16": false,
                "use_checkpoint": true,
                "qk_rms_norm": false,
                "representation_config": null
            }
        }
    },
    "dataset": {
        "name": "CTWindowSparseSDF",
        "args": {
            "resolution": 512,
            "window_type": "lung",
            "min_points": 100,
            "max_points": 100000
        }
    },
    "trainer": {
        "name": "SparseSDF_VQVAETrainer",
        "args": {
            "training_stage": 1,
            "num_workers": 30,
            "pretrained_vae_path": "./vae_weights/sparse_vae_512.pth",
            "max_steps": 100000,
            "max_epoch": 4,
            "batch_size_per_gpu": 16,
            "batch_split": 1,
            "disable_snapshot": true,
            "optimizer": {
                "name": "AdamW",
                "args": {
                    "lr": 5e-3,
                    "weight_decay": 0.01
                }
            },
            "lr_scheduler": {       
                "name": "CosineAnnealingLR",
                "args": {
                    "T_max": 100000,
                    "eta_min": 5e-5
                }
            },
            "ema_rate": [0.9999],
            "fp16_mode": "inflat_all",
            "fp16_scale_growth": 0.001,
            "grad_clip": {
                "name": "AdaptiveGradClipper",
                "args": {
                    "max_norm": 1.0,
                    "clip_percentile": 90
                }
            },
            "lambda_vq": 1.0,
            "lambda_commitment": 0.25,
            "loss_type": "l1",
            "i_print": 50,
            "i_log": 50,
            "i_sample": 50,
            "i_save": 50,
            "i_ddpcheck": 50
        }
    }
}